{
  "name": "word-datasets",
  "version": "0.1.0",
  "description": "Datasets and API for word datasets (JS/TS)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/browser.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts src/browser.ts --dts --format esm,cjs --out-dir dist && node ./scripts/copy-data.mjs",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "tsup": "^8.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
